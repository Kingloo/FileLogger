{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Information"
		}
	},
	"Kestrel": {
		"EndpointDefaults": {
			"Protocols": "Http1AndHttp2"
		},
		"Endpoints": {
			"Server": {
				"Url": "http://localhost:5000"
			}
		}
	},
	"FileLoggerOptions": {
		"Path": "..\\test.txt",
		"LogLevel": "Debug",
		"TimestampFormat": "dd/MM/yyyy HH:mm:ss zzz",
		"UseUtcTimestamp": false,
		"DrainIntervalMs": 3000,
		"DrainCount": 10
	}
}
